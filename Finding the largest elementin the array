class Main{
    public static void main(String[]args){
        int [] arr = { 21,32,25,26};
             int small = arr[0];
             for(int i =0; i<arr.length-1;i++){
                 if(arr[i+1]<small)small = arr[i+1];
                  
                 }
                 System.out.println(small);
             }
}
